{
  "name": "spyroplate",
  "version": "0.1.0",
  "description": "Spyro's awesome boilerplate",
  "main": "backend/app.js",
  "scripts": {
    "postinstall": "cd common && npm link",
    "start": "node backend/www",
    "server": "ENV=development NODE_ENV=development node backend/www",
    "backend": "ENV=development PORT=3000 nodemon --inspect backend/www",
    "client": "cd client && ENV=development webpack-dev-server --progress --colors --watch -d --config webpack.dev.config.js",
    "pre-commit-lint": "lint-staged",
    "lint:js": "eslint . --quiet --cache --rule 'prettier/prettier: [\"error\", {\"trailingComma\": \"es5\", \"singleQuote\": true}]'",
    "format": "prettier --single-quote --trailing-comma=es5 --write"
  },
  "repository": "https://github.com/spypsy/spyroplate.git",
  "author": "Spyros Psychogios <spypsy@outlook.com>",
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.12.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.11.6",
    "body-parser": "^1.15.2",
    "co": "^4.6.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.0",
    "css-loader": "^0.24.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "history": "^1.17.0",
    "invariant": "^2.2.1",
    "jsonwebtoken": "^7.1.9",
    "jwt-simple": "^0.5.0",
    "less": "^2.7.1",
    "less-loader": "^4.1.0",
    "loaders.css": "^0.1.2",
    "lodash": "^4.17.4",
    "moment": "^2.14.1",
    "mongoose": "^4.5.9",
    "mongoose-auto-increment": "^5.0.1",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "path": "^0.12.7",
    "prop-types": "^15.5.10",
    "react": "^15.3.1",
    "react-addons-shallow-compare": "^15.3.1",
    "react-datepicker": "^0.29.0",
    "react-dom": "^15.3.1",
    "react-loaders": "^2.1.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "react-table": "^6.0.5",
    "reduck": "^0.2.1",
    "redux": "^3.5.2",
    "redux-localstorage-simple": "^1.1.0",
    "redux-logger": "^3.0.6",
    "redux-object-to-promise": "^0.1.3",
    "redux-optimist": "0.0.2",
    "redux-optimist-promise": "^0.7.1",
    "redux-storage": "^4.1.1",
    "redux-storage-decorator-debounce": "^1.1.1",
    "redux-storage-decorator-filter": "^1.1.6",
    "redux-storage-engine-localstorage": "^1.1.2",
    "style-loader": "^0.13.1",
    "warning": "^3.0.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "eslint": "3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-no-not-accumulator-reassign": "^0.1.0",
    "eslint-plugin-prettier": "^2.1.2",
    "eslint-plugin-react": "^6.1.2",
    "lint-staged": "^4.0.0",
    "mocha": "^3.0.2",
    "npm": "^3.10.6",
    "prettier": "^1.4.4",
    "proxy-middleware": "^0.15.0",
    "reactify": "^1.1.1",
    "url": "^0.11.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "webpack-livereload-plugin": "^0.8.2"
  },
  "pre-commit": [
    "pre-commit-lint"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma=es5 --write",
      "eslint --quiet --rule 'prettier/prettier: [\"error\", {\"trailingComma\": \"es5\", \"singleQuote\": true}]'",
      "git add"
    ]
  }
}
